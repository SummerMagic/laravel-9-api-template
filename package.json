{
    "name": "vue-element-admin",
    "version": "4.4.0",
    "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
    "author": "meilunzhi@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build:prod": "vue-cli-service build",
        "build:stage": "vue-cli-service build --mode staging",
        "preview": "node build/index.js --preview",
        "lint": "eslint --ext .js,.vue src",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "test:ci": "npm run lint && npm run test:unit",
        "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
        "new": "plop"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "keywords": [
        "vue",
        "admin",
        "dashboard",
        "element-ui",
        "boilerplate",
        "admin-template",
        "management-system"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
    },
    "bugs": {
        "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "babel-core": "^6.26.3",
        "clipboard": "2.0.11",
        "codemirror": "^5.45.0",
        "connect-flash": "0.1.1",
        "core-js": "^3.30.2",
        "driver.js": "0.9.8",
        "dropzone": "^5.9.3",
        "echarts": "^4.9.0",
        "element-ui": "^2.15.13",
        "eslint-loader": "^4.0.2",
        "express": "^4.18.2",
        "file-saver": "^2.0.5",
        "fuse": "^0.4.0",
        "fuse.js": "^3.4.4",
        "js-cookie": "^2.2.1",
        "jsonlint": "^1.6.3",
        "jszip": "^3.10.1",
        "node-loader": "^2.0.0",
        "normalize.css": "^7.0.0",
        "nprogress": "^0.2.0",
        "passport": "^0.6.0",
        "path-to-regexp": "^2.4.0",
        "permission": "^1.1.0",
        "pinyin": "^2.9.0",
        "screenfull": "^4.2.0",
        "script-loader": "^0.7.2",
        "sortablejs": "^1.8.4",
        "tui-editor": "^1.3.3",
        "vue": "^2.7.14",
        "vue-count-to": "^1.0.13",
        "vue-i18n": "^7.8.1",
        "vue-router": "^3.6.5",
        "vue-splitpane": "^1.0.6",
        "vue-ueditor-wrap": "^2.5.6",
        "vuedraggable": "^2.24.3",
        "vuex": "^3.6.2",
        "webpack": "^4.0",
        "xlsx": "^0.17.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.19",
        "@vue/cli-plugin-eslint": "^4.5.19",
        "@vue/cli-plugin-unit-jest": "^4.5.19",
        "@vue/cli-service": "^4.5.19",
        "@vue/test-utils": "1.0.0-beta.29",
        "autoprefixer": "^9.8.8",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^23.6.0",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "chalk": "^2.4.2",
        "chokidar": "^2.1.8",
        "connect": "^3.7.0",
        "eslint": "^6.8.0",
        "eslint-plugin-vue": "^6.2.2",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.2.1",
