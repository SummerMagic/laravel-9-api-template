        "svgo": "^1.3.2",
        "vue-template-compiler": "^2.7.14"
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
